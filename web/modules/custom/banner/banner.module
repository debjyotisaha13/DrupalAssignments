<?php

/**
* Implements hook_preprocess_page(&$variables)
*/
function mytheme_preprocess_page(&$variables) {
  if (Drupal::service('path.matcher')->isFrontPage() == 1) {
    $variables['#attached']['library'][] = 'banner/slick';
  }
}

function banner_theme($existing, $type, $theme, $path) {
  return [
    'banner_template'=> ['variables'=> ['urls' => []]]
  ];
}